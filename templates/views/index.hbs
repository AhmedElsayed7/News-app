        </header>

      

  <section class="container mt-5">
    
      <div class="row justify-content-center tt ">
        {{#each Data}}
        <div class="col-lg-4 col-md-6 col-sm-12 mb-5  ">
          <div class="card h-100 wow slideInRight ">
            <img src={{this.urlToImage}} class="card-img " alt="">
            <div class=" text-center">
               <h3  >{{this.author}}</h3>
              <h5 class="card-title">{{this.title}}</h5>
              <p class="card-text wow wow bounceInUp" data-wow-duration="2s">{{this.description}}</p>
            
              <a href="{{this.url}}" class="btn btn-primary buu w-75">Check details</a>
            </div>
            
          </div>
        </div>
        {{/each}}
      </div>
   

  </section>

